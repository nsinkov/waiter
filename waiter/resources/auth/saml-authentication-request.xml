<?xml version="1.0" encoding="UTF-8"?>
<samlp:AuthnRequest AssertionConsumerServiceURL="<%= acs-url %>"
                    Destination="<%= idp-uri %>"
                    ID="<%= saml-id %>"
                    IssueInstant="<%= time-issued %>"
                    ProtocolBinding="urn:oasis:names:tc:SAML:2.0:bindings:HTTP-POST"
                    ProviderName="<%= saml-service-name %>"
                    Version="2.0"
                    xmlns:samlp="urn:oasis:names:tc:SAML:2.0:protocol">
    <saml:Issuer xmlns:saml="urn:oasis:names:tc:SAML:2.0:assertion"><%= saml-service-name %></saml:Issuer>
</samlp:AuthnRequest>
